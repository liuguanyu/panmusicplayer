{
  "name": "baiduyunmusic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "vite --port 5173 & electron .",
    "electron:build": "vite build && electron-builder",
    "electron:preview": "vite build && electron ."
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.1.2",
    "axios": "^1.6.7",
    "electron-store": "^8.2.0",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.13",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@unocss/preset-attributify": "^0.58.5",
    "@unocss/preset-icons": "^0.58.5",
    "@unocss/preset-uno": "^0.58.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "less-loader": "^12.2.0",
    "unocss": "^0.58.5",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.29.0"
  },
  "build": {
    "appId": "com.baiduyunmusic.app",
    "productName": "百度云音乐",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.music"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
